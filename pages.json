{
  "pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "首页",
        "enablePullDownRefresh": false
      }
    }, {
      "path": "pages/order/order",
      "style": {
        "navigationBarTitleText": "订单",
        "enablePullDownRefresh": false
      }
    }, {
      "path": "pages/me/me",
      "style": {
        "navigationBarTitleText": "我的",
        "enablePullDownRefresh": false
      }
    }
  ],
  "subPackages": [{
    "root": "subpkg",
    "pages": [{
      "path": "login/login",
      "style": {
        "navigationBarTitleText": "登录",
        "enablePullDownRefresh": false
      }
    }, {
      "path": "search/search",
      "style": {
        "navigationBarTitleText": "选票",
        "enablePullDownRefresh": false,
        "navigationStyle": "custom"
      }

    }, {
      "path": "progress/index",
      "style": {
        "navigationBarTitleText": "",
        "enablePullDownRefresh": false,
        "navigationStyle": "custom"
      }
    }, {
      "path": "selectCity/selectCity",
      "style": {
        "navigationBarTitleText": "",
        "enablePullDownRefresh": false
      }

    }, {
      "path": "orderDetail/orderDetail",
      "style": {
        "navigationBarTitleText": "",
        "enablePullDownRefresh": false,
        "navigationStyle": "custom"
      }

    }, {
      "path": "addPassenger/addPassenger",
      "style": {
        "navigationBarTitleText": "",
        "enablePullDownRefresh": false
      }

    }]
  }],

  "globalStyle": {
    "navigationBarTextStyle": "#eeeeee",
    "navigationBarTitleText": "武信机航",
    "navigationBarBackgroundColor": "#5a64e3",
    "backgroundColor": "#ecf3f8",
    "usingComponents": {
      "van-button": "/wxcomponents/vant/dist/button/index",
      "van-grid": "/wxcomponents/vant/dist/grid/index",
      "van-grid-item": "/wxcomponents/vant/dist/grid-item/index",
      "van-icon": "/wxcomponents/vant/dist/icon/index",
      "van-tab": "/wxcomponents/vant/dist/tab/index",
      "van-tabs": "/wxcomponents/vant/dist/tabs/index",
      "van-empty": "/wxcomponents/vant/dist/empty/index",
      "van-divider": "/wxcomponents/vant/dist/divider/index",
      "van-calendar": "/wxcomponents/vant/dist/calendar/index",
      "van-transition": "/wxcomponents/vant/dist/transition/index",
      "van-nav-bar": "/wxcomponents/vant/dist/nav-bar/index",
      "van-notice-bar": "/wxcomponents/vant/dist/notice-bar/index"
    }
  },

  "tabBar": {
    "selectedColor": "#5a64e3",
    "list": [{
        "pagePath": "pages/index/index",
        "text": "首页",
        "iconPath": "static/tab_icons/tab_1.png",
        "selectedIconPath": "static/tab_icons/tab_active_1.png"
      },
      {
        "pagePath": "pages/order/order",
        "text": "订单",
        "iconPath": "static/tab_icons/tab_2.png",
        "selectedIconPath": "static/tab_icons/tab_active_2.png"
      },
      {
        "pagePath": "pages/me/me",
        "text": "我的",
        "iconPath": "static/tab_icons/tab_3.png",
        "selectedIconPath": "static/tab_icons/tab_active_3.png"
      }
    ]
  }
