@font-face {
  font-family: "iconfont"; /* Project id  */
  src: url('data:font/truetype;charset=utf-8;base64,') format('truetype');
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-jiaohuan:before {
  content: "\e652";
}
page {
  background: #dedede;
  padding-left: 20rpx;
  padding-right: 20rpx;
  width: 750rpx;
  box-sizing: border-box;
}
.container {
  background: #fff;
}
.container .divider-wrapper {
  margin-left: 55rpx;
  margin-right: 55rpx;
}
.container .city-wrapper {
  display: flex;
  flex-direction: row;
  font-size: 54rpx;
  color: #2A2A2A;
  align-items: center;
  justify-content: space-between;
  padding-left: 52rpx;
  padding-right: 52rpx;
  box-sizing: border-box;
  background: #fff;
  margin-top: 25rpx;
  font-weight: 600;
}
.container .city-wrapper ._i {
  color: #5a64e3;
  font-size: 60rpx;
  transition: all 0.8s;
}
.container .date-wrapper {
  margin-left: 55rpx;
  margin-right: 55rpx;
  font-size: 40rpx;
  display: flex;
  align-items: baseline;
  color: #2A2A2A;
  font-weight: bold;
}
.container .date-wrapper .date-text {
  font-size: 28rpx;
  margin-left: 20rpx;
  color: #2A2A2A;
  font-weight: normal;
}
.container .search-btn-wrapper {
  margin-left: 55rpx;
  margin-right: 55rpx;
  font-weight: bold;
  letter-spacing: 3rpx;
}

